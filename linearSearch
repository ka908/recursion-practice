let arr = [-5, 2, 10, 4, 6]
function linearSearch(arr, t) {
    console.log(arr)
    for (let i = 0; i < arr.length; i++){
        if (arr[i] == t)
        {
            return i
            break
        }
        else
        {
            continue 
        }
        
    }
    return -1
}
console.log(linearSearch(arr,10));
